    <script>
        var root = new Vue
            ({
                el: '#app',
                data:
                {
                    isVueActive: 'Al-SHAFQAT',
                    User:
                    {
                        email: "",
                        password: "",
                    },
                    UserReg:
                    {
                        name: '',
                        email: '',
                        password: '',
                        dateOfBirth: '',
                        gender: 1,
                        city: '',
                        country: '',
                    },

                },
                methods:
                {
                    checkUserName(userNames) {
                        var char = userNames.which;
                        if (char > 31 && char != 32 && (char < 65 || char > 90) && (char < 97 || char > 122)) {
                            return false;
                        }

                    },
                    UserRegisteration() {
                        if (this.UserReg.country == "country") { toastr.error("Country Required", "Error!") }
                        if (this.UserReg.country != "country" && this.UserReg.country.length >= 4) { toastr.error("Invalid Country", "Error!") }
                        if (this.UserReg.city == "") { toastr.error("City Required", "Error!") }
                        if (this.UserReg.dateOfBirth == "") { toastr.error("Date of Birth Required", "Error!") }
                        if (this.UserReg.password.length <= 5) { toastr.error("Password Required Or Minimum Length(6)", "Error!"); }
                        if (this.UserReg.name == "") { toastr.error("Name Required", "Error!") }
                        if (this.UserReg.name != "" && this.UserReg.email != "" && this.UserReg.password != "" && !(this.UserReg.password.length <= 5) && this.UserReg.dateOfBirth != "" && this.UserReg.city != "" && this.UserReg.country != "" && !(this.UserReg.country.length >= 4)) {
                            var vm = this;
                            startReq();
                            axios.post('/Authentication/UserRegisteration',
                                {
                                    name: vm.UserReg.name,
                                    email: vm.UserReg.email,
                                    password: vm.UserReg.password,
                                    dateOfBirth: vm.UserReg.dateOfBirth,
                                    gender: vm.UserReg.gender,
                                    city: vm.UserReg.city,
                                    country: vm.UserReg.country,
                                })
                                .then(function (res) {
                                    if (res.data["code"] == 200 && res.data["Message"] == "User registered") {
                                        toastr.success("Please Wait", "Success!");
                                        toastr.success("Successfully Registered", "Success!");
                                        window.location = "/Dashboard/home";

                                    }
                                    if (res.data["code"] == 101 && res.data["Message"] == "Email Already Exist") {

                                        toastr.error("Email Already Exist", "Error!");
                                    }
                                    if (res.data["code"] == 500 && res.data["Message"] == "Unauthorized Changes") {

                                        toastr.error("Unauthorized Changes");
                                    }
                                    endReq();
                                })
                                .then(function (error) {
                                    console.log(error);
                                })
                        }

                    },
                    isUserValid() {
                        var vm = this;
                        if (this.User.password == "" || this.User.password.length <= 5) { toastr.error("Password Required Or Minimum Length 6", "Error!"); }
                        if (this.User.email == "") { toastr.error("Email Required", "Error!"); }
                        if (this.User.email != "" && this.User.password != "" && !(this.User.password.length <= 5)) {
                            startReq();
                            axios.post('/Authentication/isUserValid',
                                {
                                    email: vm.User.email,
                                    password: vm.User.password,
                                })
                                .then(function (res) {

                                    if (res.data["code"] == 200 && res.data["Message"] == "successfully logged in") {
                                        toastr.success("Succefully Logged in", "Success!");
                                        window.location = "/Dashboard/home";
                                    }
                                    if (res.data["code"] == 400 && res.data["Message"] == "Incorrect Email or Password") {
                                        toastr.error("Incorrect Email or Password", "Error!");
                                    }
                                    if (res.data["code"] == 400 && res.data["Message"] == "No User Exist") { toastr.error("No User Exist", "Error!"); }
                                    endReq();
                                })
                                .then(function (error) {
                                    console.log(error);
                                })
                        }
                    },

                },
            });
    </script>

